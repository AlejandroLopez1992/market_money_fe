<h1>Markets</h1>
<div id="market_table">
  <table>
    <tr>
      <th>Name</th>              
      <th>City </th>                     
      <th>State </th>
    </tr>
  <% @markets.each do |market| %>
    <tr>
      <td><%= market.name %></td>         
      <td><%=market.city %></td><td>     
      <td><%=market.state%></td>      
    </tr>
    <% end %>
  </table>
</div>